{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "classes": true
      }
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": [0, { "allow": ["warn", "error"] }]
  }
}
